{
  "name": "my-portfolio",
  "packageManager": "yarn@4.9.1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.(test|spec).(ts|tsx)"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@testing-library|framer-motion|lucide-react)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testEnvironmentOptions": {
      "customExportConditions": [
        ""
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rimraf node_modules .yarn .turbo .next build dist .cache",
    "clean:cache": "rimraf node_modules/.cache",
    "clean:dist": "rimraf dist",
    "clean:install": "yarn clean && yarn install",
    "test": "node --experimental-vm-modules --trace-warnings node_modules/jest/bin/jest.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "vercel-pull": "yarn dlx vercel pull",
    "vercel-build": "yarn dlx vercel build"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@chromatic-com/storybook": "^4.0.1",
    "@jest/source-map": "^30.0.1",
    "@storybook/addon-a11y": "^9.0.15",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/addon-links": "^9.0.15",
    "@storybook/addon-vitest": "^9.0.15",
    "@storybook/react-vite": "^9.0.15",
    "@storybook/testing-library": "^0.2.2",
    "@swc/core": "^1.12.9",
    "@swc/jest": "^0.2.38",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/identity-obj-proxy": "^3",
    "@types/jest": "29.5.5",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "canvas": "^2.5.0",
    "eslint": "^9.30.1",
    "eslint-plugin-storybook": "^9.0.15",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "^30.0.4",
    "jest-runner": "29.7.0",
    "playwright": "^1.53.2",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "storybook": "^9.0.15",
    "tailwindcss": "^4.1.11",
    "ts-jest": "29.1.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.1",
    "vitest": "^3.2.4"
  }
}